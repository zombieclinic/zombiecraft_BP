      "add": { "component_groups": ["adult","wild","tan"] }
    }
  ]


          
        
      },

      "tameable": {
        "remove": { "component_groups": ["wild"] },
        "add":    { "component_groups": ["tamed"] }
      },

      "white": { "add": { "component_groups": ["white"] } },
      "tan":   { "add": { "component_groups": ["tan"] } }
