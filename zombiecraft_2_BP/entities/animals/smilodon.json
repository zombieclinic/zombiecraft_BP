      "growup": {
        "remove": { "component_groups": ["baby"] },
        "add":    { "component_groups": ["adult"] }
      },

      "minecraft:entity_born": {
        "add": { "component_groups": ["baby","wild"] }
      },

      "minecraft:entity_spawned": {
  "randomize": [

    {
      "weight": 3,
